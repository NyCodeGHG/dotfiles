{
    "name": "spatie/laravel-webhook-server",
    "description": "Send webhooks in Laravel apps",
    "keywords": [
        "spatie",
        "laravel-webhook-server",
        "webhook",
        "server"
    ],
    "homepage": "https://github.com/spatie/laravel-webhook-server",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3|^7.3",
        "illuminate/bus": "^8.50|^9.0|^10.0",
        "illuminate/queue": "^8.50|^9.0|^10.0",
        "illuminate/support": "^8.50|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "orchestra/testbench": "^6.19|^7.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "spatie/test-time": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\WebhookServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\WebhookServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\WebhookServer\\WebhookServerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
